{
  "manifest_version": 3,
  "name": "NoMo Cache",
  "version": "1.0",
  "description": "Disable all cache with support for whitelisting certain domains.",
  "options_page": "settings.html",
  "permissions": [
    "tabs",
    "storage",
    "browsingData",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16_on.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
